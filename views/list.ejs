<%- include("header"); -%>

    <div class="boxx" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>

    <div class="boxx">
        <% for(var i=0; i<newListItem.length; i++){ %>
        <div class="item">
            <input type="checkbox">
            <p> <%= newListItem[i] %>  </p> 
        </div>
        <% } %>

        <form action="/" method="post" class="item">
            <input id="listItem" type="text" name="newItem" placeholder="New Item" autocomplete="off" autofocus>
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>

<%- include("footer"); -%>